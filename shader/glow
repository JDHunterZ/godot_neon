						<color_b>1</color_b>
						<color_a>1</color_a>
					</data>
				</item>
			</items>
			<data>
				<path>shaders\canvas_item.glsl</path>
				<uniforms>
					<uniform name="blur" type="float">
						<value>0.5</value>
					</uniform>
				</uniforms>
			</data>
		</pass>
	</pipeline>
	<objects>
		<object type="texture" path="..\icon.png">
